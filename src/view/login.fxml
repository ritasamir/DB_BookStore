<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="330.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.loginController">

    <children>
        <PasswordField fx:id="txtPassword" layoutX="38.0" layoutY="210.0" promptText="Password">
            <font>
                <Font size="18.0" />
            </font>
        </PasswordField>
        <TextField fx:id="txtUserName" layoutX="38.0" layoutY="146.0" promptText="UserName">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <Label fx:id="lblStatus" layoutX="35.0" layoutY="88.0" prefHeight="27.0" prefWidth="231.0" text="Status" textFill="#ee5645">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button fx:id="btnLogin" layoutX="119.0" layoutY="267.0" mnemonicParsing="false" onAction="#login" prefHeight="39.0" prefWidth="80.0" text="Login">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="btnRegister" layoutX="199.0" layoutY="26.0" mnemonicParsing="false" onAction="#register" prefHeight="39.0" prefWidth="101.0" text="Register">
            <font>
                <Font size="18.0" />
            </font></Button>
    </children>
</AnchorPane>
