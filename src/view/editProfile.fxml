    <Label fx:id="lblStatus" prefHeight="39.0" prefWidth="420.0" textFill="#f55e5e" GridPane.columnIndex="2" GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.rowIndex="9" GridPane.rowSpan="2">
