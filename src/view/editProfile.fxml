    <TextField fx:id="lastNameField" prefHeight="40" GridPane.columnIndex="4" GridPane.rowIndex="18" />


    <!-- Add Email Label -->
    <Label text="Email : " GridPane.columnIndex="2" GridPane.rowIndex="20">
    </Label>
    <!-- Add Email Text Field -->
    <TextField fx:id="emailField" prefHeight="40" GridPane.columnIndex="4" GridPane.rowIndex="20" />

