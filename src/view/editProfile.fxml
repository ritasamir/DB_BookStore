    <TextField fx:id="shippingAddressField" prefHeight="40" GridPane.columnIndex="4" GridPane.rowIndex="24" />

    <!-- Add old Password Label -->
    <Label text="Old Password : " GridPane.columnIndex="2" GridPane.rowIndex="26">
    </Label>
    <!-- Add old Password Field -->
    <PasswordField fx:id="oldPasswordField" prefHeight="40" GridPane.columnIndex="4" GridPane.rowIndex="26" />
    <!-- Add new Password Label -->
    <Label text="New Password : " GridPane.columnIndex="2" GridPane.rowIndex="28">
    </Label>
    <!-- Add new Password Field -->
    <PasswordField fx:id="newPasswordField" prefHeight="40" GridPane.columnIndex="4" GridPane.rowIndex="28" />


    <!-- Add Submit Button -->
    <Button fx:id="btnConfirm" defaultButton="true" onAction="#confirm" prefHeight="40" prefWidth="100" text="Submit" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="29" GridPane.rowSpan="1">
        <GridPane.margin>
            <Insets bottom="20" left="0" right="0" top="20" />
